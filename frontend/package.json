{
    "name": "cornercase-react-starter",
    "version": "0.1.0",
    "description": "",
    "engines": {
        "node": ">=8",
        "npm": ">=3"
    },
    "scripts": {
        "develop": "npm run npminstall && npm run dev-server",
        "dev-server": "babel-node tools/devServer.js",
        "npminstall": "npm install --no-progress",
        "lint": "tslint \"src/**/*.{ts,tsx}\" --project ./tsconfig.json -c tslint.json",
        "lint:fix": "tslint \"src/**/*.{ts,tsx}\" --project ./tsconfig.json --fix -c tslint.json",
        "build-prod": "babel-node tools/build.js",
        "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
        "test": "jest",
        "test:CI": "jest",
        "test:cover": "npm run test -- --coverage ",
        "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
        "test:watch": "jest --watchAll",
        "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run develop\"",
        "clean-dist": "npm run remove-dist && mkdir dist",
        "remove-dist": "rimraf ./dist",
        "build": "babel-node tools/build.js",
        "validate": "npm run lint && npm run test"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@babel/preset-env": "^7.2.3",
        "@babel/preset-react": "^7.0.0",
        "add": "^2.0.6",
        "classnames": "^2.2.6",
        "connected-react-router": "6.2.1",
        "dot-prop-immutable": "^1.5.0",
        "install": "^0.12.2",
        "jest-fetch-mock": "^2.1.0",
        "react": "16.7.0",
        "react-cookie": "^3.0.8",
        "react-dom": "16.7.0",
        "react-redux": "6.0.0",
        "react-router-dom": "4.3.1",
        "redux": "4.0.1",
        "redux-form": "^8.1.0",
        "redux-saga": "^0.16.2",
        "redux-saga-testing": "^1.0.5",
        "svg-react-loader": "^0.4.6",
        "tslint": "^5.12.0",
        "tslint-config-airbnb": "^5.11.1",
        "tslint-react": "^3.6.0",
        "typescript-fsa": "^3.0.0-beta-2",
        "whatwg-fetch": "^3.0.0"
    },
    "devDependencies": {
        "@babel/cli": "7.2.3",
        "@babel/core": "7.2.2",
        "@babel/node": "7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/plugin-transform-react-constant-elements": "^7.2.0",
        "@types/isomorphic-fetch": "^0.0.34",
        "@types/jest": "^23.3.12",
        "@types/react": "^16.7.18",
        "@types/react-dom": "^16.0.11",
        "@types/react-redux": "^6.0.12",
        "@types/redux-form": "^7.4.15",
        "@types/webpack-env": "^1.13.6",
        "assets-webpack-plugin": "^3.9.7",
        "autoprefixer": "9.4.5",
        "awesome-typescript-loader": "^5.2.1",
        "babel-core": "7.0.0-bridge.0",
        "babel-plugin-transform-react-remove-prop-types": "0.4.21",
        "chalk": "2.4.2",
        "concurrently": "4.1.0",
        "connect-history-api-fallback": "1.6.0",
        "coveralls": "3.0.2",
        "css-loader": "2.1.0",
        "cssnano": "4.1.8",
        "enzyme": "3.8.0",
        "enzyme-adapter-react-16": "1.7.1",
        "file-loader": "3.0.1",
        "hard-source-webpack-plugin": "0.13.1",
        "history": "4.7.2",
        "identity-obj-proxy": "3.0.0",
        "jest": "^23.6.0",
        "jest-cli": "^23.6.0",
        "jest-svg-transformer": "^1.0.0",
        "json-loader": "0.5.7",
        "mini-css-extract-plugin": "0.5.0",
        "mockdate": "2.0.2",
        "node-sass": "4.11.0",
        "postcss-loader": "3.0.0",
        "prop-types": "15.6.2",
        "raf": "3.4.1",
        "react-hot-loader": "4.6.3",
        "react-test-renderer": "16.7.0",
        "redux-immutable-state-invariant": "2.1.0",
        "redux-mock-store": "1.5.3",
        "replace": "1.0.1",
        "rimraf": "2.6.3",
        "sass-loader": "7.1.0",
        "source-map-loader": "^0.2.4",
        "style-loader": "0.23.1",
        "ts-jest": "^23.10.5",
        "typescript": "^3.2.2",
        "url-loader": "1.1.2",
        "webpack": "4.28.4",
        "webpack-bundle-analyzer": "3.0.3",
        "webpack-dev-server": "^3.1.14"
    },
    "keywords": [],
    "repository": {
        "type": "git",
        "url": ""
    },
    "babel": {
        "env": {
            "development": {
                "presets": [
                    "@babel/preset-env",
                    "@babel/preset-react"
                ],
                "plugins": [
                    "@babel/plugin-proposal-class-properties",
                    "react-hot-loader/babel"
                ]
            },
            "production": {
                "presets": [
                    [
                        "@babel/preset-env",
                        {
                            "targets": {
                                "ie": 9
                            },
                            "forceAllTransforms": true
                        }
                    ],
                    "@babel/preset-react"
                ],
                "plugins": [
                    "@babel/plugin-proposal-class-properties",
                    "@babel/plugin-transform-react-constant-elements",
                    "transform-react-remove-prop-types"
                ]
            }
        }
    }
}
