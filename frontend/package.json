    "babel": {
        "env": {
            "development": {
                "presets": [
                    "@babel/preset-env",
                    "@babel/preset-react"
                ],
                "plugins": [
                    "@babel/plugin-proposal-class-properties",
                    "react-hot-loader/babel"
                ]
            },
            "production": {
                "presets": [
                    [
                        "@babel/preset-env",
                        {
                            "targets": {
                                "ie": 9
                            },
                            "forceAllTransforms": true
                        }
                    ],
                    "@babel/preset-react"
