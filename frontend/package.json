{
    "name": "cornercase-react-starter",
    "version": "0.1.0",
    "description": "",
    "engines": {
        "node": ">=8",
        "npm": ">=3"
    },
    "scripts": {
        "dev": "npm run npminstall && npm run dev-server",
        "dev-server": "babel-node tools/devServer.js",
        "npminstall": "npm install --no-progress",
        "lint": "eslint ./src --ext .ts,.tsx -c ./.eslintrc.js",
        "lint:fix": "eslint ./src --ext .ts,.tsx -c ./.eslintrc.js --fix",
        "build-prod": "babel-node tools/build.js",
        "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
        "test": "jest",
        "test:CI": "jest",
        "test:cover": "npm run test -- --coverage ",
        "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
        "test:watch": "jest --watchAll",
        "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run develop\"",
        "clean-dist": "npm run remove-dist && mkdir dist",
        "remove-dist": "rimraf ./dist",
        "build": "babel-node tools/build.js",
        "validate": "npm run lint && npm run test"
    },
    "author": "",
    "license": "MIT",
